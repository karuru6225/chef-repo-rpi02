
auto lo
iface lo inet loopback

allow-hotplug eth0
iface eth0 inet manual

allow-hotplug eth1
iface eth1 inet dhcp

allow-hotplug wlan0
iface wlan0 inet manual

auto br0
iface br0 inet static
address 192.168.62.251
netmask 255.255.255.0
broadcast 192.168.62.255
gateway 192.168.62.1
bridge_ports eth0
